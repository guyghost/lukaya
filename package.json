{
  "name": "lukaya",
  "version": "1.0.0",
  "description": "Trading bot for DYDX using actor model",
  "type": "module",
  "bin": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/main.ts",
    "start": "bun run dist/main.js",
    "test": "jest",
    "test:integration": "jest --testMatch=\"**/integration/**/*.test.ts\"",
    "test:unit": "jest --testMatch=\"**/unit/**/*.test.ts\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "backtest": "bun run src/backtest/cli/run-backtest.ts",
    "api:dev": "bun run src/api/index.ts",
    "vercel-build": "npm run build",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@dydxprotocol/v4-client-js": "1.21.3",
    "cors": "^2.8.5",
    "technicalindicators": "^3.1.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/bun": "1.0.6",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "@types/cors": "^2.8.19",
    "axios": "^1.9.0",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "lodash": "4.17.21",
    "ts-jest": "^29.3.4",
    "typescript": "5.8.2",
    "vercel": "^42.3.0"
  }
}
