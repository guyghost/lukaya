{
  "version": 2,
  "functions": {
    "src/api/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["fra1"]
}
